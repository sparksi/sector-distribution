<?php


function sector_toc_preprocess_block(&$variables) {
    if($variables['elements']['#configuration']['provider'] === 'sector_toc') {
      $variables['attributes']['class'][] = 'toc';
    }
}

function sector_toc_theme() {
    return [
        'toc_block' => [
          'variables' => [
            'data' => [],
          ],
        ],
      ];
}