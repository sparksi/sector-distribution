{
    "name": "sparksinteractive/sector-distribution",
    "description": "Sector is a distribution for Drupal 8.",
    "license": "GPL-2.0-or-later",
    "type" : "drupal-profile",
    "authors": [
        {
            "name": "Sparks Interactive"
        }
    ],
    "repositories": {
        "packagist.drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset.packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal/admin_toolbar": "1.23",
        "drupal/anchor_link": "1.6",
        "drupal/autologout": "1.0",
        "drupal/better_formats": "1.x-dev",
        "drupal/better_normalizers": "1.0-beta3",
        "drupal/block_class": "1.0-alpha1",
        "drupal/bootstrap": "3.10",
        "drupal/core": "8.4.5",
        "drupal/ctools": "3.0",
        "drupal/default_content": "1.0-alpha7",
        "drupal/devel": "1.2",
        "drupal/ds": "3.1",
        "drupal/entityqueue": "1.0-alpha7",
        "drupal/facets": "1.0-beta2",
        "drupal/field_formatter_filter": "1.0",
        "drupal/field_group": "1.0",
        "drupal/field_group_link": "1.0-rc3",
        "drupal/fieldblock": "2.0-alpha1",
        "drupal/google_analytics": "2.2",
        "drupal/linkit": "5.0-beta6",
        "drupal/menu_block": "1.4",
        "drupal/metatag": "1.4",
        "drupal/password_policy": "3.0-alpha4",
        "drupal/pathauto": "1.0",
        "drupal/rabbit_hole": "1.0-beta4",
        "drupal/recreate_block_content": "1.0",
        "drupal/term_condition": "1.1",
        "drupal/token": "1.1",
        "drupal/xmlsitemap": "1.0-alpha2",
        "drupal/wysiwyg_template": "2.0-beta1"
    },
    "replace": {
      "ckeditor/templates": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/fieldblock": {
                "Add administer permission": "https://www.drupal.org/files/issues/add-administer-permission-2604972-8.patch",
                "PHP exception when placing block": "https://www.drupal.org/files/issues/php_exception_when-2810783-30.patch"
            },
            "drupal/term_condition": {
                "Blocks hidden on non-node pages": "https://www.drupal.org/files/issues/term_condition-block_hidden_not_node-2797205-7.patch",
                "Autocomplete error when editing": "https://www.drupal.org/files/issues/term_condition-2777337-1.patch"
            }
        }
    }
}
