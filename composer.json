{
    "name": "sparksinteractive/sector-distribution",
    "description": "Sector is a distribution for Drupal 8.",
    "license": "GPL-2.0-or-later",
    "type" : "drupal-profile",
    "authors": [
        {
            "name": "Sparks Interactive"
        }
    ],
    "repositories": {
        "packagist.drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset.packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal/admin_toolbar": "1.26",
        "drupal/anchor_link": "1.6",
        "drupal/autologout": "1.0",
        "drupal/backup_migrate": "4.0",
        "drupal/better_formats": "1.x-dev",
        "drupal/better_normalizers": "1.0-beta3",
        "drupal/block_class": "1.0",
        "drupal/bootstrap": "3.19",
        "drupal/captcha": "1.0-beta1",
        "drupal/config_update": "1.6",
        "drupal/core": "8.6.16",
        "drupal/ctools": "3.2",
        "drupal/default_content": "1.0-alpha8",
        "drupal/devel": "1.2",
        "drupal/diff": "1.0-rc2",
        "drupal/ds": "3.3",
        "drupal/embed": "1.0",
        "drupal/entity_embed": "1.0-beta3",
        "drupal/entityqueue": "1.0-beta2",
        "drupal/facets": "1.4",
        "drupal/field_delimiter": "1.0",
        "drupal/field_formatter_filter": "dev-1.x#200a204",
        "drupal/field_group": "1.0",
        "drupal/field_group_link": "1.0-rc3",
        "drupal/fieldblock": "2.0-alpha4",
        "drupal/google_analytics": "2.4",
        "drupal/honeypot": "1.29",
        "drupal/inline_entity_form": "1.0-rc1",
        "drupal/linkit": "5.0-beta8",
        "drupal/media_entity_browser": "2.0-alpha1",
        "drupal/menu_block": "1.5",
        "drupal/metatag": "1.8",
        "drupal/password_policy": "3.0-alpha4",
        "drupal/pathauto": "1.4",
        "drupal/rabbit_hole": "1.0-beta6",
        "drupal/recaptcha": "2.4",
        "drupal/redirect": "1.3",
        "drupal/search_api": "1.13",
        "drupal/term_condition": "1.1",
        "drupal/token": "1.5",
        "drupal/video_embed_field": "2.1",
        "drupal/view_unpublished": "1.0-alpha1",
        "drupal/views_bulk_edit": "2.2",
        "drupal/views_bulk_operations": "2.5",
        "drupal/views_menu_children_filter": "1.0-beta3",
        "drupal/webform": "5.2",
        "drupal/wysiwyg_template": "2.1",
        "drupal/xmlsitemap": "1.0-alpha3"
    },
    "replace": {
      "ckeditor/templates": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/term_condition": {
                "2948550 & 2797205 & 3010105 combined": "https://raw.githubusercontent.com/sparksi/sector-distribution/8.x-2.x/patches/term-condition-patches-2948550-3010105.patch"
            },
            "drupal/menu_block": {
                "No contextual link to edit menu": "https://www.drupal.org/files/issues/2799821-2.menu_block.contextual_links.patch",
                "Hide menu block title if empty": "https://www.drupal.org/files/issues/2018-05-08/menu_block-hide_block_if_no_links-2757215-9.patch"
            },
            "drupal/core": {
                "Profile provided configuration entities can have unmeetable dependencies": "https://www.drupal.org/files/issues/2018-09-20/2922417-25.patch"
            },
            "drupal/field_delimiter": {
                "Unfined index fix": "https://www.drupal.org/files/issues/field-deleimiter-undefined-2926377.patch"
            },
            "drupal/token": {
                "Prevent ghost menu items": "https://www.drupal.org/files/issues/token_node_preview_menu_link-2823731-4.patch"
            }
        }
    }
}
