{
    "name": "sparksinteractive/sector-distribution",
    "type": "drupal-profile",
    "description": "Sector is a distribution for Drupal 10.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sparks Interactive"
        }
    ],
    "require": {
        "cweagans/composer-patches": "~1.0",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/allowed_formats": "2.0.0",
        "drupal/block_class": "2.0.11",
        "drupal/blockgroup": "*",
        "drupal/components": "^3.0@beta",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core": "^10.1@RC",
        "drupal/ctools": "^4.0",
        "drupal/default_content": "2.0.x-dev@dev",
        "drupal/diff": "1.1",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/embed": "1.6",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/entityqueue": "1.6",
        "drupal/extlink": "1.7",
        "drupal/facets": "2.0.6",
        "drupal/field_formatter_filter": "2.0.1",
        "drupal/field_group": "3.4",
        "drupal/field_group_link": "3.1",
        "drupal/fieldblock": "2.2",
        "drupal/inline_entity_form": "1.0-rc15",
        "drupal/linkit": "^6.1@RC",
        "drupal/menu_block": "1.10",
        "drupal/metatag": "^1.23",
        "drupal/password_policy": "4.0.0",
        "drupal/pathauto": "1.11",
        "drupal/rabbit_hole": "1.0-beta11",
        "drupal/redirect": "1.8",
        "drupal/schema_metatag": "^2",
        "drupal/search_api": "^1.29",
        "drupal/sector_text_formats": "1.0.x-dev@dev",
        "drupal/sector_tonic": "^1.0",
        "drupal/svg_image": "^3.0",
        "drupal/term_condition": "^2",
        "drupal/token": "^1",
        "drupal/username_enumeration_prevention": "1.3",
        "drupal/video_embed_field": "2.5",
        "drupal/views_bulk_edit": "2.8",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_menu_children_filter": "^3.0@RC"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Permit colons in css classes": "https://www.drupal.org/files/issues/2023-05-12/permit_colons_css.patch"
            },
            "drupal/entity_usage": {
                "Better tracking per entity": "https://www.drupal.org/files/issues/2020-06-16/per-node-tracking_3151920_4.patch"
            },
            "drupal/extlink": {
                "Remove jQuery dependency & provide material symbols options": "https://www.drupal.org/files/issues/2023-05-25/material_symbols_options.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "repositories": {
        "packagist.drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset.packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
