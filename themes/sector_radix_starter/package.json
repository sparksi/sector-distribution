{
  "name": "sector_radix_starter",
  "description": "A theme based on Radix.",
  "private": true,
  "scripts": {
    "reinstall": "yarn rimraf node_modules && yarn cache clean && yarn install",
    "setup": "node .fix-theme-namespaces.js",
    "prebuild": "yarn upgrade bootstrap && yarn eslint src/**/*.js && yarn sass-lint --verbose",
    "postbuild": "yarn rimraf .sass-cache",
    "postinstall": "find node_modules/ -name '*.info.yml' -type f -delete",
    "pcss": "yarn postcss ./dist/{**,.,*}.css --replace --verbose --env production",
    "fonts": "node ./.fontmin.js",
    "generate-icons": "mkdir -p ./dist/favicons && node ./.icons.js",
    "icons": "svgo -f src/assets -r -o dist/assets",
    "scss:dev": "yarn node-sass -r ./src --output ./.sass-cache --include-path node_modules --include-path src --output-style nested --indent-type space --indent-width 2 --source-map-embed --quiet",
    "scss:production": "yarn node-sass -r ./src --output ./dist --include-path node_modules --include-path src --output-style compressed --quiet",
    "wp:build": "webpack --mode production",
    "wp:build:dev": "webpack --mode development",
    "watch:icons": "onchange 'src/assets/*' -- yarn icons",
    "watch:js": "onchange 'src/{**,.,*/**}/*.js' -- yarn wp:build:dev",
    "watch:pcss": "yarn postcss .sass-cache/{**,.,*/**}*.css --dir ./dist --base .sass-cache --verbose --watch",
    "watch:scss": "yarn scss:dev --quiet --watch",
    "watch": "yarn scss:dev && yarn npm-run-all --parallel watch:*",
    "compile": "yarn wp:build && yarn icons && yarn scss:production && yarn pcss",
    "build": "yarn run-p compile generate-icons fonts"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "mv": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "replace-in-file": "^6.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "core-js": "^3.6.5",
    "details-element-polyfill": "^2.4.0",
    "favicons": "^6.2.0",
    "fontmin": "^0.9.8",
    "imports-loader": "^0.8.0",
    "intersection-observer": "^0.11.0",
    "jquery": "^3.5.1",
    "node-sass": "^5.0.0",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-inline-svg": "^5.0.0",
    "sass-lint": "^1.13.1",
    "svgo": "^2.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-entry-plus": "^1.0.18"
  }
}
