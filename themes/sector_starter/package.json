{
	"name": "sector_starter",
	"description": "My custom sector subtheme",
	"type": "module",
	"private": true,
	"engineStrict": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"postinstall": "find node_modules/ -name '*.info' -type f -delete",
		"prebuild": "pnpm rimraf dist && pnpm tw && pnpm qa",
		"fonts": "node ./build/fonts.js",
		"tw": "tailwindcss -i src/tailwind.css -o dist/tailwind.css",
		"qa": "stylelint src/{**,.,*/**,**/**/}*.scss",
		"build": "node ./build.js",
		"watch:sync": "browser-sync start --proxy 'sector10.ddev.site' --files 'dist' '**/*.twig'",
		"watch:node": "node ./watch.js development",
		"dev": "pnpm run '/^watch:.*/'"
	},
	"devDependencies": {
		"@material-symbols/font-400": "^0.5.5",
		"@material-symbols/svg-400": "^0.5.5",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"autoprefixer": "^10.4.14",
		"browser-sync": "^2.29.3",
		"chalk": "^5.2.0",
		"chokidar": "^3.5.3",
		"esbuild": "^0.17.19",
		"fontmin": "^0.9.9",
		"glob": "^10.2.7",
		"output-file": "^2.0.2",
		"postcss": "^8.4.24",
		"postcss-inline-svg": "^6.0.0",
		"rimraf": "^5.0.1",
		"sass": "^1.63.3",
		"stylelint": "^15.7.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"tailwind-safelist-generator": "^1.0.0",
		"tailwindcss": "^3.3.2"
	}
}
